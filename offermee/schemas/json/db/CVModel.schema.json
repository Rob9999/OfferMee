{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "CVModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "freelancer_id": {
            "type": "integer",
            "title": "Freelancer ID"
        },
        "employee_id": {
            "type": "integer",
            "title": "Employee ID"
        },
        "applicant_id": {
            "type": "integer",
            "title": "Applicant ID"
        },
        "name": {
            "type": "string",
            "title": "Name"
        },
        "cv_raw_text": {
            "type": "string",
            "title": "CV Raw Text"
        },
        "cv_structured_data": {
            "type": "string",
            "title": "CV Structured Data"
        },
        "cv_schema_reference_id": {
            "type": "integer",
            "title": "CV Schema Reference ID"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x0000027F7627A840>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x0000027F7627A8E0>"
        }
    },
    "required": [
        "id",
        "name",
        "cv_raw_text",
        "cv_structured_data",
        "cv_schema_reference_id"
    ]
}