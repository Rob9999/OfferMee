{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ApplicantModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "name": {
            "type": "string",
            "title": "Name"
        },
        "capabilities_id": {
            "type": "integer",
            "title": "Capabilities ID"
        },
        "role_interest": {
            "type": "string",
            "title": "Role Interest"
        },
        "availability": {
            "type": "string",
            "title": "Availability"
        },
        "salary_interest": {
            "type": "string",
            "title": "Salary Interest"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000002A19B48E840>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000002A19B48E8E0>"
        },
        "contact_id": {
            "type": "integer",
            "title": "Contact ID"
        }
    },
    "required": [
        "id",
        "name"
    ]
}