{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "RFPModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "project_title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "location": {
            "type": "string",
            "enum": [
                "Remote",
                "OnSite",
                "Hybrid"
            ],
            "maxLength": 6
        },
        "must_have_requirements": {
            "type": "string",
            "default": "<function list at 0x000001D558544CC0>"
        },
        "nice_to_have_requirements": {
            "type": "string",
            "default": "<function list at 0x000001D558544D60>"
        },
        "tasks": {
            "type": "string",
            "default": "<function list at 0x000001D558544E00>"
        },
        "responsibilities": {
            "type": "string",
            "default": "<function list at 0x000001D558544EA0>"
        },
        "max_hourly_rate": {
            "type": "number"
        },
        "other_conditions": {
            "type": "string"
        },
        "contact_person": {
            "type": "string"
        },
        "contact_person_email": {
            "type": "string"
        },
        "provider": {
            "type": "string"
        },
        "provider_link": {
            "type": "string"
        },
        "start_date": {
            "type": "string"
        },
        "end_date": {
            "type": "string"
        },
        "duration": {
            "type": "integer"
        },
        "extension_option": {
            "type": "string",
            "enum": [
                "Yes",
                "No"
            ],
            "maxLength": 3
        },
        "original_link": {
            "type": "string"
        }
    },
    "required": [
        "id",
        "project_title",
        "must_have_requirements",
        "nice_to_have_requirements",
        "tasks",
        "responsibilities"
    ]
}