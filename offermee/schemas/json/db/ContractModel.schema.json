{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ContractModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "project_id": {
            "type": "integer",
            "title": "Project ID"
        },
        "contract_date": {
            "type": "string",
            "format": "date-time",
            "title": "Contract Date"
        },
        "signed_by_client": {
            "type": "boolean",
            "title": "Signed by Client",
            "default": false
        },
        "signed_by_contractor": {
            "type": "boolean",
            "title": "Signed by Contractor",
            "default": false
        }
    },
    "required": [
        "id",
        "contract_date"
    ]
}