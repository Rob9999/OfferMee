{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DocumentModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "related_type": {
            "type": "string",
            "enum": [
                "ADDRESS",
                "CONTACT",
                "RFP",
                "OFFER",
                "CONTRACT",
                "APPLICANT",
                "CV",
                "PROJECT",
                "INTERVIEW",
                "COMPANY",
                "EMPLOYEE",
                "FREELANCER",
                "WORKPACKAGE",
                "CAPABILITIES"
            ],
            "title": "Related Type"
        },
        "related_id": {
            "type": "integer",
            "title": "Related ID"
        },
        "document_name": {
            "type": "string",
            "title": "Document Name"
        },
        "document_link": {
            "type": "string",
            "title": "Document Link"
        },
        "document_raw_text": {
            "type": "string",
            "title": "Document Raw Text"
        },
        "document_structured_text": {
            "type": "string",
            "title": "Document Structured Text"
        },
        "document_schema_reference_id": {
            "type": "integer",
            "title": "Document Schema Reference ID"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001B2F58096C0>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001B2F5809760>"
        }
    },
    "required": [
        "id",
        "related_type",
        "related_id",
        "document_name",
        "document_link"
    ]
}