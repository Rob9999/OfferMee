{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DocumentModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "related_type": {
            "type": "string",
            "enum": [
                "ADDRESS",
                "CONTACT",
                "REQUEST",
                "OFFER",
                "CONTRACT",
                "APPLICATION",
                "CV",
                "PROJECT",
                "INTERVIEW",
                "HISTORY",
                "TASK",
                "WORKPACKAGE",
                "CERTIFICATE",
                "GRADUATION"
            ],
            "maxLength": 11
        },
        "related_id": {
            "type": "integer"
        },
        "document_name": {
            "type": "string"
        },
        "document_link": {
            "type": "string"
        },
        "document_raw_text": {
            "type": "string"
        },
        "document_structured_text": {
            "type": "string"
        },
        "document_schema_reference": {
            "type": "integer"
        },
        "created_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x00000217FFEC47C0>"
        },
        "updated_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x00000217FFEC4860>"
        }
    },
    "required": [
        "id",
        "related_type",
        "related_id",
        "document_name",
        "document_link"
    ]
}