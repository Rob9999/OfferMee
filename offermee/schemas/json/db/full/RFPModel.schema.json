{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "RFPModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "title": {
            "type": "string",
            "title": "Project Title",
            "description": "The project title"
        },
        "description": {
            "type": "string",
            "title": "Description",
            "description": "The project description (summarized, up to 20 lines)"
        },
        "location": {
            "type": "string",
            "enum": [
                "Remote",
                "OnSite",
                "Hybrid"
            ],
            "title": "Location",
            "description": "Expected working modality: Remote, OnSite or Hybrid"
        },
        "industry": {
            "type": "string",
            "enum": [
                "INFORMATION_TECHNOLOGY",
                "FINANCE",
                "HEALTHCARE",
                "EDUCATION",
                "RETAIL",
                "MANUFACTURING",
                "TRANSPORTATION",
                "ENERGY",
                "AGRICULTURE",
                "TELECOMMUNICATIONS",
                "REAL_ESTATE",
                "HOSPITALITY",
                "CONSTRUCTION",
                "ENTERTAINMENT",
                "AUTOMOTIVE",
                "FOOD_AND_BEVERAGE",
                "CHEMICALS",
                "AEROSPACE",
                "BIOTECHNOLOGY",
                "INSURANCE",
                "MEDIA",
                "LEGAL",
                "ENVIRONMENTAL",
                "SPORTS",
                "GOVERNMENT",
                "OTHER"
            ],
            "title": "Industry",
            "default": "Other",
            "description": "The industry, defaulting to Other if not specified"
        },
        "region": {
            "type": "string",
            "enum": [
                "GER_BADEN_WUERTTEMBERG",
                "GER_BAYERN",
                "GER_BERLIN",
                "GER_BRANDENBURG",
                "GER_BREMEN",
                "GER_HAMBURG",
                "GER_HESSEN",
                "GER_MECKLENBURG_VORPOMMERN",
                "GER_NIEDERSACHSEN",
                "GER_NORDRHEIN_WESTFALEN",
                "GER_RHEINLAND_PFALZ",
                "GER_SAARLAND",
                "GER_SACHSEN",
                "GER_SACHSEN_ANHALT",
                "GER_SCHLESWIG_HOLSTEIN",
                "GER_THUERINGEN",
                "DACH_GERMANY",
                "DACH_AUSTRIA",
                "DACH_SWITZERLAND",
                "EU_GERMANY",
                "EU_FRANCE",
                "EU_ITALY",
                "EU_SPAIN",
                "EU_NETHERLANDS",
                "EU_BELGIUM",
                "EU_POLAND",
                "EU_SWEDEN",
                "EU_DENMARK",
                "EU_FINLAND",
                "EU_IRELAND",
                "WORLD_EUROPE",
                "WORLD_ASIA",
                "WORLD_AFRICA",
                "WORLD_NORTH_AMERICA",
                "WORLD_SOUTH_AMERICA",
                "WORLD_OCEANIA",
                "WORLD_ANTARCTICA",
                "SPACE_LEO",
                "SPACE_MOON",
                "OTHER"
            ],
            "title": "Region",
            "default": [
                "Other",
                "Other"
            ],
            "description": "The region, defaulting to Other if not specified"
        },
        "must_have_requirements": {
            "type": "string",
            "title": "Must-have Requirements",
            "default": "<function list at 0x0000027F762D32E0>",
            "description": "Mandatory requirements for the project"
        },
        "nice_to_have_requirements": {
            "type": "string",
            "title": "Nice-to-have Requirements",
            "default": "<function list at 0x0000027F762D3380>",
            "description": "Desirable additional requirements"
        },
        "tasks": {
            "type": "string",
            "title": "Tasks",
            "default": "<function list at 0x0000027F762D3420>",
            "description": "List of tasks in the project"
        },
        "responsibilities": {
            "type": "string",
            "title": "Responsibilities",
            "default": "<function list at 0x0000027F762D34C0>",
            "description": "List of responsibilities in the project"
        },
        "max_hourly_rate": {
            "type": "number",
            "title": "Max Hourly Rate",
            "description": "Maximum hourly rate (if available)"
        },
        "other_conditions": {
            "type": "string",
            "title": "Other Conditions",
            "description": "Other conditions or requirements"
        },
        "contact_person": {
            "type": "string",
            "title": "Contact Person",
            "description": "Contact person for the project"
        },
        "contact_person_email": {
            "type": "string",
            "title": "Contact Person Email",
            "description": "Email of the contact person for the project (if available)"
        },
        "provider": {
            "type": "string",
            "title": "Provider",
            "description": "Project provider (if available)"
        },
        "provider_link": {
            "type": "string",
            "title": "Provider Link",
            "description": "Link to the project provider (if available)"
        },
        "start_date": {
            "type": "string",
            "title": "Start Date",
            "description": "Start date in format dd.mm.yyyy or mm.yyyy"
        },
        "end_date": {
            "type": "string",
            "title": "End Date",
            "description": "End date in format dd.mm.yyyy or mm.yyyy (in case of none, calculate if possible)"
        },
        "duration": {
            "type": "integer",
            "title": "Duration",
            "description": "Duration in months (in case of none, calculate if possible)"
        },
        "extension_option": {
            "type": "string",
            "enum": [
                "Yes",
                "No"
            ],
            "title": "Extension Option",
            "description": "Option to extend the project duration"
        },
        "original_link": {
            "type": "string",
            "title": "Original Link",
            "description": "Link to the original source of the project"
        },
        "source": {
            "type": "string",
            "enum": [
                "ONLINE",
                "EMAIL",
                "MANUAL"
            ],
            "title": "Source",
            "description": "The RFP Source, must be one of [ONLINE, EMAIL, MANUAL]"
        },
        "status": {
            "type": "string",
            "enum": [
                "NEW",
                "OFFERED",
                "OUTDATED",
                "REJECTED"
            ],
            "title": "Status",
            "default": "NEW",
            "description": "The RFP Status, must be one of [NEW, OFFERED, OUTDATED, REJECTED]"
        }
    },
    "required": [
        "id",
        "title",
        "industry",
        "region",
        "must_have_requirements",
        "nice_to_have_requirements",
        "tasks",
        "responsibilities",
        "source",
        "status"
    ]
}