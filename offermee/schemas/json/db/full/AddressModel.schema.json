{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "AddressModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "street": {
            "type": "string"
        },
        "city": {
            "type": "string"
        },
        "zip_code": {
            "type": "string"
        },
        "country": {
            "type": "string"
        },
        "documents": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "DocumentModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "related_type": {
                        "type": "string",
                        "enum": [
                            "ADDRESS",
                            "CONTACT",
                            "REQUEST",
                            "OFFER",
                            "CONTRACT",
                            "APPLICATION",
                            "CV",
                            "PROJECT",
                            "INTERVIEW",
                            "COMPANY",
                            "EMPLOYEE",
                            "WORKPACKAGE",
                            "CAPABILITIES"
                        ],
                        "maxLength": 12
                    },
                    "related_id": {
                        "type": "integer"
                    },
                    "document_name": {
                        "type": "string"
                    },
                    "document_link": {
                        "type": "string"
                    },
                    "document_raw_text": {
                        "type": "string"
                    },
                    "document_structured_text": {
                        "type": "string"
                    },
                    "document_schema_reference_id": {
                        "type": "integer"
                    },
                    "created_at": {
                        "type": "string",
                        "default": "<function datetime.utcnow at 0x000001A10CF2CC20>"
                    },
                    "updated_at": {
                        "type": "string",
                        "default": "<function datetime.utcnow at 0x000001A10CF2CCC0>"
                    }
                },
                "required": [
                    "id",
                    "related_type",
                    "related_id",
                    "document_name",
                    "document_link"
                ]
            }
        },
        "histories": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "HistoryModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "related_type": {
                        "type": "string",
                        "enum": [
                            "ADDRESS",
                            "CONTACT",
                            "REQUEST",
                            "OFFER",
                            "CONTRACT",
                            "APPLICATION",
                            "CV",
                            "PROJECT",
                            "INTERVIEW",
                            "COMPANY",
                            "EMPLOYEE",
                            "WORKPACKAGE",
                            "CAPABILITIES"
                        ],
                        "maxLength": 12
                    },
                    "related_id": {
                        "type": "integer"
                    },
                    "description": {
                        "type": "string"
                    },
                    "event_date": {
                        "type": "string"
                    },
                    "created_by": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string",
                        "default": "<function datetime.utcnow at 0x000001A10CF2E2A0>"
                    }
                },
                "required": [
                    "id",
                    "related_type",
                    "related_id",
                    "event_date",
                    "created_by"
                ]
            }
        }
    },
    "required": [
        "id",
        "street",
        "city",
        "zip_code",
        "country"
    ]
}