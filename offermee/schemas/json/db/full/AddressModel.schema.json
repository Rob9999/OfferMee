{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "AddressModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "street": {
            "type": "string",
            "title": "Street"
        },
        "city": {
            "type": "string",
            "title": "City"
        },
        "zip_code": {
            "type": "string",
            "title": "Zip Code"
        },
        "country": {
            "type": "string",
            "title": "Country"
        },
        "documents": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "DocumentModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "ID",
                        "readOnly": true
                    },
                    "related_type": {
                        "type": "string",
                        "enum": [
                            "ADDRESS",
                            "CONTACT",
                            "REQUEST",
                            "OFFER",
                            "CONTRACT",
                            "APPLICATION",
                            "CV",
                            "PROJECT",
                            "INTERVIEW",
                            "COMPANY",
                            "EMPLOYEE",
                            "WORKPACKAGE",
                            "CAPABILITIES"
                        ],
                        "title": "Related Type"
                    },
                    "related_id": {
                        "type": "integer",
                        "title": "Related ID"
                    },
                    "document_name": {
                        "type": "string",
                        "title": "Document Name"
                    },
                    "document_link": {
                        "type": "string",
                        "title": "Document Link"
                    },
                    "document_raw_text": {
                        "type": "string",
                        "title": "Document Raw Text"
                    },
                    "document_structured_text": {
                        "type": "string",
                        "title": "Document Structured Text"
                    },
                    "document_schema_reference_id": {
                        "type": "integer",
                        "title": "Document Schema Reference ID"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At",
                        "readOnly": true,
                        "default": "<function datetime.utcnow at 0x000001D6D9256840>"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Updated At",
                        "readOnly": true,
                        "default": "<function datetime.utcnow at 0x000001D6D92568E0>"
                    }
                },
                "required": [
                    "id",
                    "related_type",
                    "related_id",
                    "document_name",
                    "document_link"
                ]
            }
        },
        "histories": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "HistoryModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "ID",
                        "readOnly": true
                    },
                    "related_type": {
                        "type": "string",
                        "enum": [
                            "ADDRESS",
                            "CONTACT",
                            "REQUEST",
                            "OFFER",
                            "CONTRACT",
                            "APPLICATION",
                            "CV",
                            "PROJECT",
                            "INTERVIEW",
                            "COMPANY",
                            "EMPLOYEE",
                            "WORKPACKAGE",
                            "CAPABILITIES"
                        ],
                        "title": "Related Type"
                    },
                    "related_id": {
                        "type": "integer",
                        "title": "Related ID"
                    },
                    "description": {
                        "type": "string",
                        "title": "Description"
                    },
                    "event_date": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Event Date"
                    },
                    "created_by": {
                        "type": "string",
                        "title": "Created By",
                        "readOnly": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At",
                        "readOnly": true,
                        "default": "<function datetime.utcnow at 0x000001D6D9257EC0>"
                    }
                },
                "required": [
                    "id",
                    "related_type",
                    "related_id",
                    "event_date",
                    "created_by"
                ]
            }
        }
    },
    "required": [
        "id",
        "street",
        "city",
        "zip_code",
        "country"
    ]
}