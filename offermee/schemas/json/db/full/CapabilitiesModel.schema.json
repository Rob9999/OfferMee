{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "CapabilitiesModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "roles": {
            "type": "string",
            "title": "Roles"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000002A19B443C40>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000002A19B443D80>"
        },
        "soft_skills": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "SkillModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "ID",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "type": {
                        "type": "string",
                        "title": "Type"
                    },
                    "capabilities_soft": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "CapabilitiesModel (Already Included)"
                        }
                    },
                    "capabilities_tech": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "CapabilitiesModel (Already Included)"
                        }
                    }
                },
                "required": [
                    "id",
                    "name",
                    "type"
                ]
            }
        },
        "tech_skills": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "SkillModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "ID",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "type": {
                        "type": "string",
                        "title": "Type"
                    },
                    "capabilities_soft": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "CapabilitiesModel (Already Included)"
                        }
                    },
                    "capabilities_tech": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "title": "CapabilitiesModel (Already Included)"
                        }
                    }
                },
                "required": [
                    "id",
                    "name",
                    "type"
                ]
            }
        },
        "documents": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "DocumentModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "ID",
                        "readOnly": true
                    },
                    "related_type": {
                        "type": "string",
                        "enum": [
                            "ADDRESS",
                            "CONTACT",
                            "REQUEST",
                            "OFFER",
                            "CONTRACT",
                            "APPLICATION",
                            "CV",
                            "PROJECT",
                            "INTERVIEW",
                            "COMPANY",
                            "EMPLOYEE",
                            "WORKPACKAGE",
                            "CAPABILITIES"
                        ],
                        "title": "Related Type"
                    },
                    "related_id": {
                        "type": "integer",
                        "title": "Related ID"
                    },
                    "document_name": {
                        "type": "string",
                        "title": "Document Name"
                    },
                    "document_link": {
                        "type": "string",
                        "title": "Document Link"
                    },
                    "document_raw_text": {
                        "type": "string",
                        "title": "Document Raw Text"
                    },
                    "document_structured_text": {
                        "type": "string",
                        "title": "Document Structured Text"
                    },
                    "document_schema_reference_id": {
                        "type": "integer",
                        "title": "Document Schema Reference ID"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At",
                        "readOnly": true,
                        "default": "<function datetime.utcnow at 0x000002A19B417880>"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Updated At",
                        "readOnly": true,
                        "default": "<function datetime.utcnow at 0x000002A19B417920>"
                    }
                },
                "required": [
                    "id",
                    "related_type",
                    "related_id",
                    "document_name",
                    "document_link"
                ]
            }
        },
        "histories": {
            "type": "array",
            "items": {
                "$schema": "http://json-schema.org/draft-07/schema#",
                "title": "HistoryModel",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "ID",
                        "readOnly": true
                    },
                    "related_type": {
                        "type": "string",
                        "enum": [
                            "ADDRESS",
                            "CONTACT",
                            "REQUEST",
                            "OFFER",
                            "CONTRACT",
                            "APPLICATION",
                            "CV",
                            "PROJECT",
                            "INTERVIEW",
                            "COMPANY",
                            "EMPLOYEE",
                            "WORKPACKAGE",
                            "CAPABILITIES"
                        ],
                        "title": "Related Type"
                    },
                    "related_id": {
                        "type": "integer",
                        "title": "Related ID"
                    },
                    "description": {
                        "type": "string",
                        "title": "Description"
                    },
                    "event_date": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Event Date"
                    },
                    "created_by": {
                        "type": "string",
                        "title": "Created By"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At",
                        "readOnly": true,
                        "default": "<function datetime.utcnow at 0x000002A19B440F40>"
                    }
                },
                "required": [
                    "id",
                    "related_type",
                    "related_id",
                    "event_date",
                    "created_by"
                ]
            }
        }
    },
    "required": [
        "id"
    ]
}