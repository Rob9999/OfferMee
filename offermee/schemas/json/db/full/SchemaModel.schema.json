{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "SchemaModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "name": {
            "type": "string",
            "title": "Name"
        },
        "description": {
            "type": "string",
            "title": "Description"
        },
        "schema_definition": {
            "type": "string",
            "title": "Schema Definition"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001AAE42D79C0>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001AAE42D7B00>"
        }
    },
    "required": [
        "id",
        "name",
        "schema_definition"
    ]
}