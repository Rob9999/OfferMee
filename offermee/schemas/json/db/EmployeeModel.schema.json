{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "EmployeeModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "name": {
            "type": "string",
            "title": "Name"
        },
        "capabilities_id": {
            "type": "integer",
            "title": "Capabilities ID"
        },
        "role": {
            "type": "string",
            "title": "Role"
        },
        "start_date": {
            "type": "string",
            "format": "date-time",
            "title": "Start Date"
        },
        "end_date": {
            "type": "string",
            "format": "date-time",
            "title": "End Date"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x0000027F7629C220>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x0000027F7629C2C0>"
        },
        "contact_id": {
            "type": "integer",
            "title": "Contact ID"
        }
    },
    "required": [
        "id",
        "name",
        "role",
        "start_date"
    ]
}