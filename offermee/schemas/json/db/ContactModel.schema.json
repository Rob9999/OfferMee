{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ContactModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "first_name": {
            "type": "string"
        },
        "last_name": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "type": {
            "type": "string",
            "enum": [
                "BUSINESS",
                "EMPLOYEE",
                "FREELANCER",
                "APPLICANT",
                "COMPANY",
                "SUPPLIER"
            ],
            "maxLength": 10
        },
        "address_id": {
            "type": "integer"
        },
        "created_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x0000025BAFE53CE0>"
        },
        "updated_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x0000025BAFE53D80>"
        }
    },
    "required": [
        "id",
        "first_name",
        "last_name",
        "phone",
        "email",
        "type"
    ]
}