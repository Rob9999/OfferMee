{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ContactModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "first_name": {
            "type": "string",
            "title": "First Name"
        },
        "last_name": {
            "type": "string",
            "title": "Last Name"
        },
        "phone": {
            "type": "string",
            "title": "Phone"
        },
        "email": {
            "type": "string",
            "title": "Email"
        },
        "type": {
            "type": "string",
            "enum": [
                "BUSINESS",
                "EMPLOYEE",
                "FREELANCER",
                "APPLICANT",
                "COMPANY",
                "SUPPLIER"
            ],
            "title": "Type"
        },
        "address_id": {
            "type": "integer",
            "title": "Address ID"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000002DC5460A020>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000002DC5460A0C0>"
        }
    },
    "required": [
        "id",
        "first_name",
        "last_name",
        "phone",
        "email",
        "type"
    ]
}