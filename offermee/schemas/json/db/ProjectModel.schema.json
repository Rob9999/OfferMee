{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ProjectModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "title": {
            "type": "string",
            "title": "Title"
        },
        "description": {
            "type": "string",
            "title": "Description"
        },
        "location": {
            "type": "string",
            "title": "Location"
        },
        "must_haves": {
            "type": "string",
            "title": "Must Haves"
        },
        "nice_to_haves": {
            "type": "string",
            "title": "Nice to Haves"
        },
        "tasks": {
            "type": "string",
            "title": "Tasks"
        },
        "responsibilities": {
            "type": "string",
            "title": "Responsibilities"
        },
        "hourly_rate": {
            "type": "number",
            "title": "Hourly Rate"
        },
        "other_conditions": {
            "type": "string",
            "title": "Other Conditions"
        },
        "contact_person": {
            "type": "string",
            "title": "Contact Person"
        },
        "contact_person_email": {
            "type": "string",
            "title": "Contact Person Email"
        },
        "provider": {
            "type": "string",
            "title": "Provider"
        },
        "provider_link": {
            "type": "string",
            "title": "Provider Link"
        },
        "start_date": {
            "type": "string",
            "format": "date-time",
            "title": "Start Date"
        },
        "end_date": {
            "type": "string",
            "format": "date-time",
            "title": "End Date"
        },
        "duration": {
            "type": "string",
            "title": "Duration"
        },
        "extension_option": {
            "type": "string",
            "title": "Extension Option"
        },
        "original_link": {
            "type": "string",
            "title": "Original Link"
        },
        "status": {
            "type": "string",
            "enum": [
                "NEW",
                "OFFER_SENT",
                "INTERVIEW",
                "CONTRACT_SIGNED",
                "IN_PROGRESS",
                "COMPLETED",
                "REJECTED"
            ],
            "title": "Status",
            "default": "NEW"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001AAE43A4CC0>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001AAE43A4D60>"
        },
        "address_id": {
            "type": "integer",
            "title": "Address ID"
        }
    },
    "required": [
        "id",
        "title",
        "start_date"
    ]
}