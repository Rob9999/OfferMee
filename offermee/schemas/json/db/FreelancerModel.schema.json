{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "FreelancerModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "name": {
            "type": "string",
            "title": "Name"
        },
        "website": {
            "type": "string",
            "title": "Website"
        },
        "capabilities_id": {
            "type": "integer",
            "title": "Capabilities ID"
        },
        "role": {
            "type": "string",
            "title": "Role"
        },
        "availability": {
            "type": "string",
            "title": "Availability"
        },
        "desired_rate_min": {
            "type": "number",
            "title": "Desired Rate Min"
        },
        "desired_rate_max": {
            "type": "number",
            "title": "Desired Rate Max"
        },
        "offer_template": {
            "type": "string",
            "title": "Offer Template"
        },
        "preferred_language": {
            "type": "string",
            "title": "Preferred Language"
        },
        "preferred_currency": {
            "type": "string",
            "title": "Preferred Currency"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001D6D92A8040>"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At",
            "readOnly": true,
            "default": "<function datetime.utcnow at 0x000001D6D92A80E0>"
        },
        "contact_id": {
            "type": "integer",
            "title": "Contact ID"
        }
    },
    "required": [
        "id",
        "name",
        "website",
        "role"
    ]
}