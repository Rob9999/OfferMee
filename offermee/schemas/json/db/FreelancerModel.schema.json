{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "FreelancerModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "website": {
            "type": "string"
        },
        "capabilities_id": {
            "type": "integer"
        },
        "role": {
            "type": "string"
        },
        "availability": {
            "type": "string"
        },
        "desired_rate_min": {
            "type": "number"
        },
        "desired_rate_max": {
            "type": "number"
        },
        "offer_template": {
            "type": "string"
        },
        "preferred_language": {
            "type": "string"
        },
        "preferred_currency": {
            "type": "string"
        },
        "created_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x000001D5584F8720>"
        },
        "updated_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x000001D5584F87C0>"
        },
        "contact_id": {
            "type": "integer"
        }
    },
    "required": [
        "id",
        "name",
        "website",
        "role"
    ]
}