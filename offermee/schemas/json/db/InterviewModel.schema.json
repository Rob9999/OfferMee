{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "InterviewModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "title": "ID",
            "readOnly": true
        },
        "project_id": {
            "type": "integer",
            "title": "Project ID"
        },
        "date": {
            "type": "string",
            "format": "date-time",
            "title": "Date"
        },
        "interviewer": {
            "type": "string",
            "title": "Interviewer"
        },
        "notes": {
            "type": "string",
            "title": "Notes"
        },
        "rating": {
            "type": "number",
            "title": "Rating"
        }
    },
    "required": [
        "id",
        "date",
        "interviewer"
    ]
}