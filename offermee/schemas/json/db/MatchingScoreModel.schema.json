{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "MatchingScoreModel",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "project_id": {
            "type": "integer"
        },
        "freelancer_id": {
            "type": "integer"
        },
        "matching_score": {
            "type": "number",
            "default": 0.0
        },
        "match_details": {
            "type": "string"
        },
        "matched_at": {
            "type": "string",
            "default": "<function datetime.utcnow at 0x000001C3D22FA5C0>"
        }
    },
    "required": [
        "id",
        "project_id",
        "freelancer_id",
        "matching_score",
        "matched_at"
    ]
}